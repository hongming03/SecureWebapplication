<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ReverseProxyInboundRule1" stopProcessing="false">
                    <match url="(.*)" />
                    <action type="Rewrite" url="http://localhost:5000/{R:1}" />
                </rule>
                <rule name="RequestBlockingRule1" patternSyntax="ExactMatch" stopProcessing="true">
                    <match url=".*" />
                    <conditions>
                        <add input="{REMOTE_ADDR}" pattern="169.254.42.46" />
                    </conditions>
                    <action type="CustomResponse" statusCode="403" statusReason="Forbidden: Access is denied." statusDescription="You do not have permission to view this directory or page using the credentials that you supplied." />
                </rule>
            </rules>
        </rewrite>
        <security>
            <ipSecurity>
                <remove ipAddress="169.254.42.46" subnetMask="255.255.255.255" domainName="" />
                <add ipAddress="169.254.42.46" allowed="false" />
                <add ipAddress="169.254.42.22" allowed="false" />
            <add ipAddress="123.123.123.123" allowed="false" /><add ipAddress="169.254.42.46" allowed="false" /><add ipAddress="169.254.42.46" allowed="false" /><add ipAddress="169.254.42.46" allowed="false" /><add ipAddress="169.254.42.46" allowed="false" /></ipSecurity>
        </security>
    </system.webServer>
    <location path="" overrideMode="Deny">
    </location>
    <location path="" overrideMode="Allow">
    <appSettings file="" />
    </location>
</configuration>